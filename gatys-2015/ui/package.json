{
  "dependencies": {
    "@tensorflow/tfjs": "^0.14.1",
    "@tensorflow/tfjs-node": "^0.2.1",
    "@tensorflow/tfjs-vis": "^0.4.0",
    "antd": "^3.11.6",
    "assets": "^3.0.1",
    "css": "^2.2.4",
